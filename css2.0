h1,p,h2,h3 {
    text-align: center;
    margin-bottom: 30px;
}
body {
    font-family:  Tahoma, Geneva, Verdana, sans-serif;
    line-height: 1.6;
    margin: 0;
    padding: 0;
    background-color: #91a850;
    color: #ffffff;
}
.gallery {
    display: flex;
    gap: 10px;
    flex-wrap: wrap;
    margin: 20px 0;
    justify-content: center;
}
footer {
    text-align: center;
    padding: 20px;
    background-color: #4B5320;
    color: #ffffff;
    margin-top: 40px;
}
